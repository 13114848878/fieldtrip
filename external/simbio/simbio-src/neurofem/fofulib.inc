     * JAC23 ,JACI21,JACI22,JACI23,JAC31 ,JAC32 ,JAC33 ,JACI31,JACI32,
     * JACI33, 
     * REDLIN,REDINT,IVOGAU,IVODRE,IVOTET,MKNEL ,LQZANF,LQZEND,LQKSTA,
     * LQKDRE,LQKVIE,LQKTET,LQKKEI,LQKQUA
C
      SAVE /FOFUCO/

